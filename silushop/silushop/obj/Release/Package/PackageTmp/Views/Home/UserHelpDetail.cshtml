
@{
    Layout = null;
}
@model RelexBarDLL.UserHelp
<!DOCTYPE html>

<html>
<head>
    @Html.Action("Header", "Static")
    <title>@Model.Title</title>
</head>
<body class="bgf4f4f4">
    <div class="header">
        <h3>@Model.Title</h3>
        <a onclick="window.history.back()" title=""><i class="icon-chevron-thin-left"></i></a>
    </div>
    <div class="titlelist">
        @Html.Raw(Model.Content)
    </div>
</body>
</html>
