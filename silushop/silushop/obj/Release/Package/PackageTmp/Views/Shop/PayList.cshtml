@model List<RelexBarBLL.Models.AdminPayListModel>
@for (int i = 0; i < Model.Count; i++)
{
    <tr>
        <th>@Model[i].CardNumber</th>
        <th>@Model[i].Name</th>
        @*<th>@Model[i].OPhone</th>
            <th>@Model[i].OName</th>*@
        <th>@(Model[i].InOut == (int)RelexBarBLL.Common.enPayInOutType.Out ? "-" : "")@Model[i].Val</th>
        <th>@Model[i].Remark</th>
        <th>@Model[i].CreateTime</th>
    </tr>
}
<script>createFooter(@ViewData["sum"])</script>

