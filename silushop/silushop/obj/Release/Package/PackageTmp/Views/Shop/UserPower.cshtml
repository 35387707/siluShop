
@{
    Layout = null;
    var list = new RelexBarBLL.UserHelpBLL().GetListByType(4);
    RelexBarDLL.Users u = Session["user"] as RelexBarDLL.Users;
    var ot = new RelexBarBLL.UsersBLL().CanYashangTihuo(u.ID);
}

@model RelexBarDLL.Users
<!DOCTYPE html>

<html>
<head>
    @Html.Action("Header", "Static")
    <title>会员特权</title>
</head>
<body class="bgf4f4f4">
    <div class="header">
        <h3>会员特权</h3>
        <a onclick="window.history.back()" title=""><i class="icon-chevron-thin-left"></i></a>
    </div>
    <div class="expenseslist">
        <ul id="paylist">
            @for (int i = 0; i < list.Count; i++)
            {
                <li>
                    <a href="/Home/UserHelpDetail/@list[i].ID" title="">
                        @list[i].Title >>
                    </a>
                </li>
            }
            <li>
                <a href="/Home/Index" title="">
                    <span></span><h5>升级成为牙商 >></h5><div>
                        <span></span><p></p>
                    </div>
                </a>
            </li>
            @if (ot != null)
            {
                <li>
                    <a href="/Home/OrderConfirm?id=@ot.ID" title="">
                        <span></span><h5>我要提货 >></h5><div>
                            <span></span><p></p>
                        </div>
                    </a>
                </li>
            }
        </ul>
    </div>
    <div class="footnav">
        @Html.Action("ShopFooter", "Static", new { id = 4 })
    </div>
</body>
</html>
