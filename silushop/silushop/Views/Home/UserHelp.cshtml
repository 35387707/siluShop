
@{
    Layout = null;
}
@model List<RelexBarDLL.UserHelp>
<!DOCTYPE html>

<html>
<head>
    @Html.Action("Header", "Static")
    <title>UserHelp</title>
</head>
<body class="bgf4f4f4">
    <div class="header">
        <h3>
            @switch (ViewData["id"].ToString())
            {
                case "1": Write("关于我们"); break;
                case "2": Write("帮助中心"); break;
                case "3": Write("丝路商学院"); break;
                case "4": Write("会员特权"); break;
            }
        </h3>
        <a onclick="window.history.back()" title=""><i class="icon-chevron-thin-left"></i></a>
    </div>
    <div class="titlelist">
        <ul class="clearfix">
            @for (int i = 0; i < Model.Count; i++)
            {
                <li><a href="/Home/UserHelpDetail/@Model[i].ID" title="">@Model[i].Title<i class="icon-chevron-thin-left arrow"></i></a></li>
            }
        </ul>
    </div>
</body>
</html>
