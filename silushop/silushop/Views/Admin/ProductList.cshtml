@model List<RelexBarBLL.Models.ProductListModel>
@for (int i = 0; i < Model.Count; i++)
{
    <tr>
        <td>@Model[i].Name【@Model[i].Number】</td>
        <td><img width="100" src="@Model[i].Img"/></td>
        <td>@Model[i].Price</td>
        <td>@Model[i].Stock</td>
        <td>
            <input type="button" class="btn btn-danger" value="@((Model[i].Status==1)?"起售":"禁售")" onclick="CStatus('@Model[i].ID',this)" />
        </td>
        <td>@Model[i].CreateTime</td>
        <td><input type="button" class="btn btn-danger"  value="编辑" onclick="EditProduct('@Model[i].ID')"/></td>
    </tr>
}
<script>createFooter(@ViewData["sum"])</script>
